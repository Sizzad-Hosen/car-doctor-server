{
    "name": "my-vercel-project",
    "version": 2,
    "builds": [
      {
        "src": "public/**",
        "use": "@vercel/static-build",
        "config": { "zeroConfig": true }
      }
    ],
    "routes": [
      { "src": "/api/*", "dest": "/api/:path" }
    ],
    "env": {
      "API_KEY": "my-api-key",
      "DEBUG": "true"
    }
  }
  